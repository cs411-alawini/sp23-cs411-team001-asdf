<!-- users.ejs -->
<!DOCTYPE html>
<html>
<head>
  <title>Users</title>
</head>
<body>
  <h1>Users</h1>
  <table>
    <thead>
      <tr>
        <th>Video Name</th>
        <th>View Count</th>
     </tr>
    </thead>
    <tbody>
      <% for (var i = 0; i < users.length; i++) { %>
        <tr>
          <td><%= users[i].VideoTitle %></td>
          <td><%= users[i].TotalViews %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</body>
</html>
