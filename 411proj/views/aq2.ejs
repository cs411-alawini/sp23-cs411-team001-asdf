<%- include("partials/header"); -%>
<body>
  <style>
    h1 {
      font-size: 24px;
      color: #333;
      text-align: center;
    }
    table {
      width: 100%;
      border-collapse: collapse;
      margin-top: 20px;
    }
    th, td {
      padding: 10px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }
    th {
      background-color: #f2f2f2;
    }
  </style>
  <h1>Results</h1>
  <table>
    <thead>
      <tr>
        <th>Channel Name</th>
	<th>Number of Videos</th>
	<th>View Count</th>
      </tr>
    </thead>
    <tbody>
      <% for (var i = 0; i < aq2result.length; i++) { %>
        <tr>
          <td><%= aq2result[i].ChannelTitle %></td>
          <td><%= aq2result[i].NumOfVideos %></td>
	  <td><%= aq2result[i].TotalViews %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</body>
</html>
